{
    "name": "Training: Impoved Dark Theme",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Extension for forum.training-server.com. Changing default dark theme to new.",
    "icons": {
        "16": "icons/icon16x16.png",
        "32": "icons/icon32x32.png",
        "48": "icons/icon48x48.png",
        "128": "icons/icon128x128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://forum.training-server.com/*"],
            "js": ["style.js"],
            "run_at": "document_end"
        }
    ]
}